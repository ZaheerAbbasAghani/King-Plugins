        jQuery(document).ready(function ($) {
            let cookieName = 'popup_cookie';
            let $modal = $("#popup-modal");

            $modal.modal({
                backdrop: true,
                keyboard: true,
                show: false
            });

            $modal.on('hide.bs.modal', function (e) {
                Mage.Cookies.set(cookieName, 1);
            });

            function hasClosedClosureModal() {
                let cookie = Mage.Cookies.get(cookieName);
                return cookie !== null;
            }

            if (!hasClosedClosureModal()) {
                $modal.modal("show");
            }

            $('#readMorePopUp').click(function() {
                $modal.modal("show");
            });

            $( "#popupForm" ).submit(function( event ) {
                return ValidateEmail();
            });

            function ValidateEmail()
            {
                var email = document.getElementById('emailAddress').value;
                const re = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
                console.log(email)
                if (re.test(String(email).toLowerCase()))
                {
                    Mage.Cookies.set(cookieName, 1);
             //       $modal.modal("hide");
                    return true;
                }

                alert("You have entered an invalid email address!")
                return false;
            }

        });
